{
  "name": "Chrome Extension For Study",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://news.yahoo.co.jp/*"],
      "js": [
        "js/app.js"
      ]
    }
  ],
  "background" : {
    "service_worker" : "js/background.js"
  },
  "permissions": [
    "notifications",
    "tabs"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "html/popup.html"
  },
  "manifest_version": 3
}
